

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - fontys_rockstars_frontend</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" type="text/css" href="~/Font/MyFontsWebfontsKit/MyFontsWebfontsKit.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rle="stylesheet" href="https://use.fontawesome.com/release/v5.4.1/css/all.css" />
</head>
<body>
    <header>
        <div class="navigation">
            <nav class="navbar navbar-expand-md navbar-light">
                <div class="container-fluid">
                    <a class="navbar-brand" href="#">
                        <img src="~/Images/logo.png" alt="" class="d-inline-block align-text-top">
                    </a>
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="collapse navbar-collapse" id="navbarNav">
                        <ul class="navbar-nav">
                            <li class="nav-item">
                                <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Pravicy</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link text-dark" asp-area="" asp-controller="Survey" asp-action="Survey">Survey</a>
                            </li>
                            <li class="nav-item">
                                <div class="searchb">
                                    <form id="f_search" method="get" asp-action="Search" class="searchForm">
                                        <input id="ipt_search" type="text" class="SearchBar" name="SearchTerm" placeholder="" pattern="\S+.*" autocomplete="off" value="@ViewData["KeyWord"]" />
                                        <button class="btn"><i class="searchIcon fa fa-search"></i></button>
                                    </form>
                                </div>
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>
        </div>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
            <button id="topBtn"><i class="fa fa-arrow-up"></i></button>
        </main>
    </div>

    <footer class="text-center text-lg-start footy">
        <div class="container p-4">
            <div class="row">
                <div class="col-lg-6 col-md-12 mb-4 mb-md-0">
                    <h5 class="text-uppercase">Team Rockstars IT</h5>

                    <p>
                        Hoe verbind je de beste developers van Nederland aan je en hoe behoud je ze? Team Rockstars IT werd geboren. De weg naar happy developers is doordrenkt met hartelijkheid, openheid, energie, eigenzinnigheid en heel veel ambitie!
                    </p>
                </div>
                <div class="col-lg-3 col-md-6 mb-4 mb-md-0">
                    <h5 class="text-uppercase">MEER INFORMATIE</h5>

                    <ul class="list-unstyled mb-0">
                        <li>
                            <a href="#!" class="text-dark">Over ons</a>
                        </li>
                        <li>
                            <a href="#!" class="text-dark">Onze werkwijze</a>
                        </li>
                        <li>
                            <a href="#!" class="text-dark">Ons team</a>
                        </li>
                        <li>
                            <a href="#!" class="text-dark">Werken bij</a>
                        </li>
                    </ul>
                </div>

                <div class="col-lg-3 col-md-6 mb-4 mb-md-0">
                    <h5 class="text-uppercase">CHAPTERS</h5>

                    <ul class="list-unstyled mb-0">
                        <li>
                            <a href="#!" class="text-dark">Overzicht Chapters (regio hubs)</a>
                        </li>
                        <li>
                            <a href="#!" class="text-dark">Chapter North</a>
                        </li>
                        <li>
                            <a href="#!" class="text-dark">Chapter Central</a>
                        </li>
                        <li>
                            <a href="#!" class="text-dark">Chapter West</a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="text-center p-3" style="background-color: rgba(0, 0, 0, 0.2);">
            &copy; 2021 Copyright:
            <a class="text-dark" href="https://Google.com/">Rockstars IT</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    <script>
        $(function () {
            $('#f_search input').keydown(function (e) {

                var searchTerm = $("#ipt_search").val();

                if (searchTerm == "" && event.keyCode === 13) {
                    e.preventDefault();
                    return false;
                }
            });
        })
    </script>
    @await RenderSectionAsync("Scripts", required: false)


</body>
</html>
